@{

    var session = (AlgoOpp.Models.Membership)Session["model"];
    if (session == null)
    {
        Response.Redirect("~/CollegeRegister/Login");
    }
    if (string.Compare(session?.Est_Type, "Company", true) == 0)
    {
        Response.Redirect("~/CompanyRegister/Login");
    }

}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="navbar navbar-expand-lg navbar-light fixed-top" style="background-color:darkslategray; font-family: 'Bookman Old Style';">
        <div class="container-fluid">
            <img class=".navbar-brand" src="~/Content/Logo2.png" />
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarScroll" aria-controls="navbarScroll" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" style="color:black; float:right;">
                    <li class="nav-item nav-link">@Html.ActionLink("Dashboard", "DashBoard", "CollegeRegister")</li>
                    <li class="nav-item nav-link">@Html.ActionLink("Student Details", "Index", "StudentDetails")</li>
                    <li class="nav-item nav-link">@Html.ActionLink("Current Openings", "Companies", "CollegeRegister")</li>
                    <li class="nav-item nav-link">@Html.ActionLink("Student Status", "StudentStatus", "CollegeRegister")</li>

                    <li class="dropdown ms-auto mb-2 mb-lg-0">
                        <a class=" dropdown-toggle" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" href="javascript:;">Notifcation</a>
                        @{ Html.RenderAction("Notification", "CollegeRegister"); }
                    </li>

                    <li class="nav-item"><a href="@Url.Action("Logout","CollegeRegister")">LogOut</a></li>

                </ul>
            </div>
        </div>
    </div>
    <style>
        img {
            width: 200px;
            height: 50px;
        }
    </style>

    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>